{
    "name": "yangsuda/slimcms",
    "type": "project",
    "description": "The SlimCMS framework",
    "keywords": [
        "slimCMS",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "http://github.com/yangsuda/slimcms",
    "license": "proprietary",
    "authors": [
        {
            "name": "zhucy",
            "email": "919579@qq.com",
            "homepage": "https://weibo.com/u/3215596351"
        }
    ],
    "require": {
        "php": "^7.2",
        "yangsuda/framework": "2.0.0.x-dev",
        "yangsuda/slimcms-public": "dev-main",
        "oomphinc/composer-installers-extender": "2.0.0"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
    "extra":
    {
        "installer-types": ["public"],
        "installer-paths":
        {
            "public/":["yangsuda/slimcms-public"]
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    }
}
