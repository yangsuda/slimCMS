{
    "name": "slim/slim-skeleton",
    "description": "A SlimCMS Framework application for rapid development",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Lockhart",
            "email": "info@joshlockhart.com",
            "homepage": "http://www.joshlockhart.com/"
        },
        {
            "name": "Pierre Berube",
            "email": "pierre@lgse.com",
            "homepage": "http://www.lgse.com/"
        }
    ],
    "require": {
        "ext-json": "*",
        "monolog/monolog": "^2.0",
        "php": "^7.2",
        "php-di/php-di": "^6.1",
        "slim/psr7": "^1.1",
        "slim/slim": "^4.5",
        "dapphp/securimage": "3.6.7"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SlimCMS\\": "src/",
            "App\\": "app/"
        },
        "files": [
            "src/function/Core.php"
        ]
    }
}
